syntax = "v1"

type (
	// 城市信息
	CityInfo {
		Code string `json:"code"` // 城市代码
		Name string `json:"name"` // 城市名称
	}
	// 获取城市列表响应
	GetCitiesRes {
		Cities []CityInfo `json:"cities"` // 城市列表
	}
)

service dictionary {
	@doc "获取城市列表"
	@handler GetCities
	get /api/dictionary/cities returns (GetCitiesRes)
}

